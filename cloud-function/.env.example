PASSWORD_RESET_URL='http://localhost/#/reset-password?token={jwt}'
PASSWORD_RESET_SECRET=someSecretInProduction
EMAIL_HOST=mail.example.com
EMAIL_PORT=465
EMAIL_SECURE=true
