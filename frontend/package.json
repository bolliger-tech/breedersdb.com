{
  "name": "breedersdb",
  "version": "0.0.1",
  "description": "Collect and Analyze your Plant's Traits.",
  "productName": "BreedersDB",
  "author": "bolliger.tech <mail@bolliger.tech>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build && bun run sentry:sourcemaps",
    "serve": "quasar serve dist/spa",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org bolligertech --project breedersdb ./dist && sentry-cli sourcemaps upload --org bolligertech --project breedersdb ./dist",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@sentry/cli": "^2.31.0",
    "@sentry/vue": "^7.109.0",
    "@urql/exchange-request-policy": "^1.1.0",
    "@urql/exchange-retry": "^1.2.1",
    "@urql/vue": "^1.1.3",
    "dotenv": "^16.4.5",
    "gql.tada": "^1.4.3",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.4.18",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.9.1",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.0.0-beta.5",
    "@quasar/cli": "^2.4.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
