{
  "name": "breedersdb",
  "version": "0.0.1",
  "description": "Collect and Analyze your Plant's Traits.",
  "productName": "BreedersDB",
  "author": "bolliger.tech <mail@bolliger.tech>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "tsc": "vue-tsc --noEmit",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "VITE_BDB_VERSION=$(git rev-parse HEAD) quasar dev -m pwa",
    "build": "VITE_BDB_VERSION=${VITE_BDB_VERSION:-$(git rev-parse HEAD)} quasar build -m pwa",
    "build:sentry": "yarn run build && yarn run sentry:sourcemaps",
    "serve": "quasar serve dist/pwa --port 9200",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org bolligertech --project breedersdb ./dist && sentry-cli sourcemaps upload --org bolligertech --project breedersdb ./dist",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "graphql:schema": "gql.tada generate-schema --output='./src/graphql.graphql' http://localhost/api/hasura/v1/graphql",
    "graphql:output": "gql.tada generate-output",
    "color:primary": "node scripts/primary-color.js",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.17",
    "@sentry/vue": "^8.34.0",
    "@urql/exchange-request-policy": "^1.2.0",
    "@urql/exchange-retry": "^1.3.0",
    "@urql/vue": "^1.4.1",
    "blueimp-load-image": "^5.16.0",
    "gql.tada": "^1.8.10",
    "quasar": "^2.18.0",
    "quircs-wasm": "^0.1.1",
    "register-service-worker": "^1.7.2",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.2",
    "vue-router": "^4.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@eslint/js": "^9.21.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.1",
    "@quasar/cli": "^2.5.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "@sentry/cli": "^2.37.0",
    "@types/blueimp-load-image": "^5.16.6",
    "@types/node": "^20.16.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "typescript": "~5.5.4",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.2.8",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "trustedDependencies": [
    "@sentry/cli",
    "esbuild",
    "vue-demi"
  ]
}
